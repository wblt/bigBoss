.container{
  height: auto;
}
.container .search{
  width: 735rpx;
  height: 65rpx;
  padding: 12.5rpx 0 12.5rpx 15rpx;
  background: #ffa200;
}
.container .search-left{
  flex: 8;
  background: #ffffff;
  text-align: left;
}
.container .search-left input{
  height: 65rpx;
  font-size: 26rpx;
  float: left;
}
/* .search-placeholder{
  color: #000000;
  line-height: 20rpx;
} */
.container .search .search-left image{
  float: left;
  width: 35rpx;
  height: 35rpx;
  padding: 15rpx 15rpx 15rpx 20rpx;
}
.container .search .search-right{
  flex: 1;
}
.container .search .search-right image{
  width: 45rpx;
  height: 45rpx;
  padding: 10rpx;
}

.container{
  padding: 0;
  font-size: 14rpx;
  background: #F0F4F3;
  color: #000;
}